{   
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "semi":"off",
        "no-console":"off",
        "no-alert":"off",
        "no-plusplus": "off",
        "max-len":"off",
        "indent": ["off", "tab"],
        "linebreak-style": ["error", "windows"]
    }
}